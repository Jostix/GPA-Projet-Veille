<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HtmlHttpUtils</servlet-name>
        <servlet-class>ch.hearc.ig.gpa.servlet.HtmlHttpUtils</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletFil</servlet-name>
        <servlet-class>ch.hearc.ig.gpa.servlet.ServletFil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update</servlet-name>
        <servlet-class>ch.hearc.ig.gpa.servlet.Update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletFilVeilleTechno</servlet-name>
        <servlet-class>ch.hearc.ig.gpa.servlet.ServletFilVeilleTechno</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletFilVeilleProtection</servlet-name>
        <servlet-class>ch.hearc.ig.gpa.servlet.ServletFilVeilleProtection</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HtmlHttpUtils</servlet-name>
        <url-pattern>/HtmlHttpUtils</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletFil</servlet-name>
        <url-pattern>/ServletFil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update</servlet-name>
        <url-pattern>/Update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletFilVeilleTechno</servlet-name>
        <url-pattern>/ServletFilVeilleTechno</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletFilVeilleProtection</servlet-name>
        <url-pattern>/ServletFilVeilleProtection</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>ServletFil</welcome-file>
    </welcome-file-list>
</web-app>
